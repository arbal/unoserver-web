{
  "name": "unoserver-web",
  "version": "3.8.3",
  "description": "Provides API for Unoserver",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.13.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  },
  "prettier": "@lynxtaa/prettier-config",
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "scripts": {
    "prebuild": "rimraf build/*",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node -r dotenv-safe/config build/index.js",
    "dev": "tsc-watch --onSuccess \"cross-env NODE_ENV=development node -r dotenv-safe/config build/index.js\"",
    "validate": "npm run lint && npm run check-format && npm run check-types",
    "lint": "eslint --ext .js,.ts src",
    "prettier": "prettier 'src/**/*'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "check-types": "tsc --noEmit",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "content-disposition": "^0.5.4",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "execa": "^6.1.0",
    "fastify": "^3.27.4",
    "fastify-cors": "^6.0.3",
    "fastify-multer": "^2.0.2",
    "fastify-swagger": "^5.0.0",
    "http-errors": "^2.0.0",
    "mime-types": "^2.1.35",
    "p-queue": "7.2.0",
    "p-retry": "^5.1.0",
    "pino": "^7.9.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@lynxtaa/eslint-config": "^0.1.5",
    "@lynxtaa/prettier-config": "^0.1.1",
    "@types/content-disposition": "^0.5.4",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.26",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "formdata-node": "^4.3.2",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.5.1",
    "node-fetch": "^3.2.3",
    "pino-pretty": "^7.5.4",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.2"
  }
}
